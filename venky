import java.util.*;

import java.lang.*;
import java.io.*;


class Ideone 


 {
	
public static void main (String[] args) throws java.lang.Exception

	
           {
	
     
 
                   Scanner reader = new Scanner(System.in);


     
		    int num = reader.nextInt();


        
			if(num > 0)
        
			 {
         
        
				if(num % 2 == 0)
 
          
					 System.out.println(" even");

        
				else
 
           
					 System.out.println("odd");
   
		      }
         
          
			  if(num<0)
   	
       
		  System.out.println( " invalid");
	
	
		}


}
